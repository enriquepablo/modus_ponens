fact        = _{ SOI ~ rfact ~ EOI }

rfact        = { value ~ pred ~ value }

var         = @{ ("<" ~ &ASCII_ALPHA_UPPER) ~ ASCII_ALPHA_UPPER ~ ASCII_ALPHA* ~ ('0'..'9')* ~ ">" }

pred        = @{ "ISA" ~ ('0'..'9')+ | "IS" ~ ('0'..'9')+ }

v_name      = @{ ASCII_ALPHANUMERIC+ }

value       = _{ v_name | var }

WHITESPACE  = { (" " | "\t" | NEWLINE)+ }
